## Copy this file to `.env.local` and fill with real values.
## Use non-secret placeholders here to avoid secret scanner alerts.

# Optional: if you ever add Dev.to integration
DEV_TO_API_KEY = example_devto_key

# Used server-side in getStaticProps for GitHub data
GITHUB_API_KEY = example_github_token

# Public username used on client
NEXT_PUBLIC_GITHUB_USERNAME = your_github_username

# Chatbot backend (Hugging Face Space URL, no trailing slash)
CHATBOT_API_BASE_URL = https://<username>-<space-name>.hf.space
NEXT_PUBLIC_CHATBOT_API_BASE_URL = https://<username>-<space-name>.hf.space

# Notion API integration used by /api/contact (server-side only)
NOTION_API_TOKEN = example_notion_token
NOTION_DATABASE_ID = example_notion_db_id
